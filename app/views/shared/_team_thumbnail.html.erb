<% Branch.all.each do |b| %>

  <div class="team_thumbnail">
    <%= b.name %>
    <span class="subtext">
      <%= b.rank %>
      <br/><%= b.buds %> <i class="fa fa-file"></i>
      &nbsp; <%= b.leaves %> <i class="fa fa-home"></i>
      &nbsp; <%= b.branches %> <i class="fa fa-user"></i>
  </span>
    <span class="expand">
      <i class="fa fa-angle-down"></i>
    </span>

  </div>

<% end %>
